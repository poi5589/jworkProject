<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "mybatis-3-config.dtd" >
<configuration>
	
	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	
	<typeAliases>
		<typeAlias type="com.mj.jwork.approval.model.vo.Approval" alias="Approval" />
		<typeAlias type="com.mj.jwork.employee.model.vo.Employee" alias="Employee" />
		<typeAlias type="com.mj.jwork.ess.model.vo.Attendence" alias="Attendence" />
		<typeAlias type="com.mj.jwork.mail.model.vo.Mail" alias="Mail" />
		<typeAlias type="com.mj.jwork.project.model.vo.Project" alias="Project" />
		<typeAlias type="com.mj.jwork.reservation.model.vo.Car" alias="Car" />
		<typeAlias type="com.mj.jwork.reservation.model.vo.CarReservation" alias="CarReservation" />
		<typeAlias type="com.mj.jwork.reservation.model.vo.CfrReservation" alias="CfrReservation" />
		<typeAlias type="com.mj.jwork.reservation.model.vo.CfRoom" alias="CfRoom" />
		<typeAlias type="com.mj.jwork.reservation.model.vo.CfrImg" alias="CfrImg" />
	</typeAliases>
	<!-- 유의사항 : 미리 등록해두려면 해당 클래스가 진짜 존재해야함! -->
	
	<!-- 연결할 db에 대한 정보는 root-context.xml 파일에 작성 예정 -->
	
	<mappers>
		<mapper resource="/mappers/approval-mapper.xml" />
		<mapper resource="/mappers/employee-mapper.xml" />
		<mapper resource="/mappers/ess-mapper.xml" />
		<mapper resource="/mappers/mail-mapper.xml" />
		<mapper resource="/mappers/project-mapper.xml" />
		<mapper resource="/mappers/reservation-mapper.xml" />
	</mappers>
	<!-- 유의사항 : 미리 등록해두려면 해당 mapper 파일이 진짜 존재하고, namespace 도 지정되어 있어야함 -->

</configuration>
